@{
    ViewData["Title"] = "Produto";
        

}

<div>
    <h1>Página de Clientes</h1>
    <h2>Conteúdo novo para o projeto</h2>
    <p>Acesse: <a href="https://www.microsoft.com/pt-br">Microsoft</a>.</p>
    <p>Acesse: <a href="https://www.apple.com/br/">Apple</a>.</p>
    <h4>Selecionamos este cliente especial @ViewBag.Nome</h4>

    <hr />
    <p>Agora veja todos os nossos clientes</p>
    <ul>
        @foreach(var cli in ViewBag.todosCli)
        {
            <li>@cli</li>
        }
    </ul>
</div>